{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js", // Points to your Vite configuration file
      "use": "@vercel/static-build" // Use the Vercel static build plugin
    }
  ],
  "routes": [
    {
      "src": "/(.*)", // Match all routes
      "dest": "/index.html" // Serve index.html for all routes (for client-side routing)
    }
  ]
}